            "}"
        ],
        "description": "DefaultProperties Block"
    },
    "StructDefaultProperties Block": {
        "prefix": "structdefaultproperties",
        "body": [
            "structdefaultproperties",
            "{",
            "\t$TM_SELECTED_TEXT$0",
            "}"
        ],
        "description": "StructDefaultProperties Block"
    },
    "Archetype Block": {
        "prefix": "begin object",
        "body": [
            "begin object name=${1:Name} class=${2:class}",
            "\t$TM_SELECTED_TEXT$0",
            "end object"
        ],
        "description": "Archetype Block"
    }
